generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  image     String?
  oauth     String
  instagram String?
  linkedIn  String?
  twitter   String?
  facebook  String?
  createdAt DateTime @default(now())
}
